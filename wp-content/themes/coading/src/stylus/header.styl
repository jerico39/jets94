/*　ナビ(sp)　*/
nav.gnavi
  position fixed
  z-index  2
  top   0
  left  0
  color #000
  background #fff
  text-align left
  transform translateY(-100%)
  transition all 0.6s
  width 100%
  height 100%
  padding 0px
  //display none
  img
    display none
  .search
    margin 10px 10px 10px 70px
    border-bottom 0px solid #000
    display flex;
    form
      display inline-block
      margin 0 10px 0 0
  .search a
    margin 0px 6px 0 6px
    padding-top 0px
    color: #000;
  ul.accordion>li
    padding 10px 10px 5px 5px
    border-bottom 1px solid #000
  ul.accordion>li.head
    padding-top 10px
    padding-bottom 10px
    padding-left 35px
    a
      font-weight: bold
  ul.accordion>li p
    margin 7px 5px 7px 10px
  ul.small-list>li
    padding 10px
    border-bottom 1px solid #000
  ul.small-list>li>p:first-child
    margin 0
  ul 
    display block 
    background #fff
    width 100%
    padding 0px
    p
      font-weight bold
    li
      list-style-type none
    li:hover
      background #ddd
    li a 
      color #000
      text-decoration none
nav.active 
  display block

/* このクラスを、jQueryで付与・削除する */
header nav.gnavi.active 
  transform: translateY(0%)

/*　end ナビ(sp)　*/

header
  .header-right
    display none


/*====================================================================
nav.gnavi .accordion
====================================================================*/

nav.gnavi
  ul.accordion>li
    position: relative;
    z-index: +1;
    cursor: pointer;
    transition-duration: 0.2s;
    padding: 0 0 0 0;
  ul.accordion>li:hover
    opacity .8
  ul.accordion>li .i_box
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    top: 70%;
    left: 1%;
    width: 20px;
    height: 20px;
    border: 1px solid #000;
    margin-top: -20px;
    box-sizing: border-box;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    transform-origin: center center;
    transition-duration: 0.2s;
  ul.accordion>li .i_box .one_i
    display: block;
    width: 18px;
    height: 18px;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    transform-origin: center center;
    transition-duration: 0.2s;
    position: relative;

  ul.accordion>li .i_box .one_i:before,
  ul.accordion>li .i_box .one_i:after
    display: flex;
    content: '';
    background-color: #000;
    border-radius: 10px;
    width: 18px;
    height: 4px;
    position: absolute;
    top: 7px;
    left: 0;
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
    transform-origin: center center;
  ul.accordion>li .i_box .one_i:before
    width: 4px;
    height: 18px;
    top: 0;
    left: 7px;
  ul.accordion .small-list
    display: none;
    box-sizing: border-box;
  ul.accordion>li.open .i_box
    -webkit-transform: rotate(-360deg);
    transform: rotate(-360deg);
  ul.accordion>li.open .i_box .one_i:before 
    content: none;
  ul.accordion>li.open .i_box .one_i:after 
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);


