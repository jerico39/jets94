  .header-search
    box-sizing: border-box;
    position: relative;
    border: 1px solid #999;
    display: block;
    padding: 3px 10px;
    border-radius: 20px;
    height: 2.3em;
    width: 265px;
    overflow: hidden;
    background-color: #fff

    input[type="text"]
      border: none;
      height: 2.0em;

    input[type="text"]:focus 
      outline: 0;

    input[type="submit"]
      cursor: pointer;
      font-family: FontAwesome;
      font-size: 1.3em;
      border: none;
      background: none;
      color: #3879D9;
      position: absolute;
      width: 2.5em;
      height: 2.5em;
      right: 0;
      top: -10px;
      outline : none;
      background-color: #fff
    button 
      /* 背景色を無色に */
      background: transparent
      /* 枠線を消す */
      border: none
      /* クリックした際に枠線をnone消す */
      outline: none
      /* 影を消す */
      box-shadow: none


/*　ハンバーガーボタン(sp)　*/
header
  .btn-gnavi
    display  block
    position fixed
    z-index  3
    left  20px
    top    15px
    width  42px
    height 42px
    cursor pointer
    text-align center
    span
      display  block
      position absolute
      width   30px
      height  2px
      left    6px
      background #fff
      -webkit-transition 0.3s ease-in-out
      -moz-transition    0.3s ease-in-out
      transition         0.3s ease-in-out
  .btn-gnavi span:nth-child(1)
    top 10px
  .btn-gnavi span:nth-child(2)
    top 20px
  .btn-gnavi span:nth-child(3)
    top 30px
  /* ナビ開いてる時のボタン */
  .btn-gnavi.active span
    background #000
  .btn-gnavi.active span:nth-child(1)
    top  16px
    left 6px
    -webkit-transform rotate(-45deg)
    -moz-transform   rotate(-45deg)
    transform         rotate(-45deg)
  
  .btn-gnavi.active span:nth-child(2),
  .btn-gnavi.active span:nth-child(3) {
    top: 16px;
    -webkit-transform: rotate(45deg);
    -moz-transform   : rotate(45deg);
    transform        : rotate(45deg);
  }

/* end ハンバーガーボタン(sp)　*/

