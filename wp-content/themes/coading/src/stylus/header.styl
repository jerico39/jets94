base-color = #115740
second-color = #108f62
list-line-color = #e8e8e8

/* ローディング画面 */
/* Loading Block */
#loading 
  width 100vw
  height 100vh
  transition all 1s
  background-color base-color
  position fixed
  top 0
  left 0
  z-index 9999

.spinner 
  width 100px
  height 100px
  margin 200px auto
  background-color #fff
  border-radius 100%
  animation sk-scaleout 1.0s infinite ease-in-out

/* Loading Animation */
@keyframes sk-scaleout {
  0% {
    transform: scale(0);
  } 100% {
    transform: scale(1.0);
    opacity: 0;
  }
} 
/* Hide Loading Block */
.loaded 
  opacity 0
  visibility hidden


header
  background-color  base-color  
  padding 5px 0px 0px 0px
  position fixed
  z-index  20
  width 100%
  .header-logo
    padding 5px 0 0 0
    text-align center
  .header-contents //中央揃え
    margin 0 auto
    text-align center
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
    justify-content: space-around;
  .header-menu
    display: none;
  .header-sns
    margin-left: auto; //display:flexと合わせて使用することで以降右寄せになる。
    ul
      text-align right
      padding-left 0px
      li
        display: inline-block;
        text-align center
        text-align right
    a
      color: #fff;
      margin 0 5px
      text-align right
 /* 
  .header-search
    margin 0 0 0 0px
    padding 15px 0 0 0
  
  .header-search form input[type="text"],nav.gnavi form input[type="text"]
      width 153px
      height 33px
      border-radius: 0px
      margin 0 0 0 0
      padding 0 0 0 0
    input[type="text"]:focus
      outline 0
  .header-search button,nav.gnavi button
    border-radius 0px
    cursor pointer
    font-family FontAwesome
    border none
